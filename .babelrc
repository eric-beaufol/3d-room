  "presets": [
    [
      "@babel/preset-env", {
        "targets": {
          "browsers": ["last 2 versions"]
        },
        "modules": false
      }
    ],
    "@babel/preset-react"
  ],
  "plugins": [
    "react-hot-loader/babel",
    "transform-class-properties"
  ],
  "compact": false
}